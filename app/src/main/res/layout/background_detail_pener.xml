<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:id="@+id/RelativeContent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.cardview.widget.CardView
                android:id="@+id/card1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="5dp"
                app:cardElevation="5dp"
                app:cardUseCompatPadding="false">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingVertical="10dp"
                    android:paddingStart="10dp">

                    <TextView
                        android:id="@+id/tvTglpurchase"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        android:text="07 Januari 2021"
                        android:textColor="@color/black" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="20dp">

                        <TextView
                            android:id="@+id/tv_lblnopurchase"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:text="No Pusrchase"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tv_titikd"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tv_lblnopurchase"
                            android:gravity="center"
                            android:paddingHorizontal="5dp"
                            android:text=":"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tvPurchase"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tv_titikd"
                            android:paddingEnd="5dp"
                            android:text="SBY-0012-11"
                            android:textColor="@color/black" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/tv_lblarea"
                            android:layout_width="95dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:text="Area"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tv_titikd4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tv_lblarea"
                            android:gravity="center"
                            android:paddingHorizontal="5dp"
                            android:text=":"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tvArea"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tv_titikd4"
                            android:paddingEnd="5dp"
                            android:text="Surabaya"
                            android:textColor="@color/black" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/tvLblsts"
                            android:layout_width="95dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:text="Status"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tv_titikd6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tvLblsts"
                            android:gravity="center"
                            android:paddingHorizontal="5dp"
                            android:text=":"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tvSts"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tv_titikd6"
                            android:paddingEnd="5dp"
                            android:text="Belum Diterima"
                            android:textColor="@color/black" />
                    </RelativeLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/card1"
                android:orientation="vertical"
                app:cardElevation="5dp"
                app:cardUseCompatPadding="false">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="10dp">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card2_0"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardUseCompatPadding="false">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/tvlblDetailItem"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignBottom="@+id/btnImageAddItem"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:gravity="center_vertical"
                                android:paddingEnd="5dp"
                                android:text="Detail Item"
                                android:textAllCaps="true"
                                android:textColor="@color/black" />

                            <ImageView
                                android:id="@+id/btnImageAddItem"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:background="@drawable/btn_action"
                                android:backgroundTint="@color/white"
                                app:srcCompat="@drawable/ic_baseline_add_circle_24" />

                            <TextView
                                android:id="@+id/tvLine1"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_below="@id/tvlblDetailItem"
                                android:layout_marginTop="5dp"
                                android:background="#9F818181" />

                            <TextView
                                android:id="@+id/tvItemNotFound"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/tvLine1"
                                android:layout_centerHorizontal="true"
                                android:layout_marginVertical="10dp"
                                android:text="Tidak Ada Item"
                                android:visibility="gone" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/tvLine1"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="5dp"
                                android:orientation="vertical"
                                android:visibility="visible">

                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="10dp"
                                    app:cardElevation="3dp"
                                    app:cardUseCompatPadding="false">

                                    <androidx.constraintlayout.widget.ConstraintLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginBottom="16dp">

                                        <RelativeLayout
                                            android:id="@+id/relativeLayoutbg"
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="8dp"
                                            app:layout_constraintEnd_toEndOf="parent"
                                            app:layout_constraintStart_toStartOf="parent"
                                            app:layout_constraintTop_toTopOf="parent">

                                            <TextView
                                                android:id="@+id/tvQtyPener"
                                                android:layout_width="wrap_content"
                                                android:layout_height="0dp"
                                                android:layout_alignTop="@id/tvSatuanPener"
                                                android:layout_alignBottom="@id/tvSatuanPener"
                                                android:layout_marginTop="0dp"
                                                android:layout_marginEnd="5dp"
                                                android:layout_toStartOf="@+id/tvSatuanPener"
                                                android:gravity="center_vertical"
                                                android:text="100"
                                                android:textColor="@color/black" />

                                            <TextView
                                                android:id="@+id/tvNmItemPener"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentLeft="true"
                                                android:layout_marginStart="16dp"
                                                android:layout_marginEnd="16dp"
                                                android:text="Mie Sedap"
                                                android:textColor="@color/black" />

                                            <TextView
                                                android:id="@+id/tvSatuanPener"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_below="@id/tvHrgPener"
                                                android:layout_alignEnd="@+id/tvHrgPener"
                                                android:paddingVertical="10dp"
                                                android:text="Box"
                                                android:textColor="@color/black" />

                                            <TextView
                                                android:id="@+id/tvHrgPener"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginEnd="16dp"
                                                android:layout_toStartOf="@+id/imgEditbg"
                                                android:gravity="right"
                                                android:text="Rp. 600.000"
                                                android:textColor="@color/black" />

                                            <ImageView
                                                android:id="@+id/imgEditbg"
                                                android:layout_width="24dp"
                                                android:layout_height="24dp"
                                                android:layout_above="@id/imgHpsbg"
                                                android:layout_alignParentEnd="true"
                                                app:srcCompat="@drawable/ic_baseline_create_24_primary" />

                                            <ImageView
                                                android:id="@+id/imgHpsbg"
                                                android:layout_width="24dp"
                                                android:layout_height="24dp"
                                                android:layout_alignBottom="@id/tvSatuanPener"
                                                android:layout_alignParentEnd="true"
                                                android:layout_marginBottom="5dp"
                                                app:srcCompat="@android:drawable/ic_delete" />

                                            <TextView
                                                android:id="@+id/line2"
                                                android:layout_width="0dp"
                                                android:layout_height="1dp"
                                                android:layout_below="@id/tvSatuanPener"
                                                android:layout_alignStart="@id/tvTtlHrgPener"
                                                android:layout_alignEnd="@id/imgEditbg"
                                                android:layout_marginTop="5dp"
                                                android:background="#858585" />

                                            <TextView
                                                android:id="@+id/tvTtlHrgPener"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_below="@id/line2"
                                                android:layout_marginTop="10dp"
                                                android:layout_marginEnd="16dp"
                                                android:layout_toStartOf="@+id/imgEditbg"
                                                android:gravity="right"
                                                android:text="Rp. 600.000.000"
                                                android:textColor="@color/black" />

                                        </RelativeLayout>
                                    </androidx.constraintlayout.widget.ConstraintLayout>

                                </androidx.cardview.widget.CardView>

                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>

                    <TextView
                        android:id="@+id/tvNote"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/card2_0"
                        android:layout_marginTop="10dp"
                        android:text="Note"
                        android:textColor="@color/black" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/layoutInput2"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tvNote"
                        android:layout_centerHorizontal="true"
                        android:enabled="false"
                        android:visibility="visible">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/tietNote"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:enabled="false"
                            android:gravity="top"
                            android:inputType="textMultiLine"
                            android:minHeight="100dp"
                            android:paddingHorizontal="15dp"
                            tools:ignore="SpeakableTextPresentCheck" />

                    </com.google.android.material.textfield.TextInputLayout>

                </RelativeLayout>

            </androidx.cardview.widget.CardView>


        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/BottomSheetbg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/card3Pener"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="-25dp"
            app:cardCornerRadius="30dp"
            app:cardElevation="20dp"
            app:cardUseCompatPadding="false">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:background="@color/white"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:id="@+id/linearBottomsheetbg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone"
                    app:layout_constraintBottom_toTopOf="@id/linearLayoutTllHrg"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="end"
                            android:padding="10dp">

                            <TextView
                                android:id="@+id/tvSupplierBottom"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/tv_titikd8"
                                android:gravity="end"
                                android:paddingEnd="5dp"
                                android:text="Pt. Philips"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/tv_titikd8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/tvLblSupplier"
                                android:gravity="center"
                                android:paddingHorizontal="5dp"
                                android:text=":"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/tvLblSupplier"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:text="Supplier"
                                android:textColor="@color/black" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/lytAla"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="end"
                            android:padding="10dp">

                            <TextView
                                android:id="@+id/tvAlamatBottom"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/tv_titikd10"
                                android:gravity="end"
                                android:paddingEnd="5dp"
                                android:text="Jl. Ikan paus no23"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/tv_titikd10"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/tvLblAlamat"
                                android:gravity="center"
                                android:paddingHorizontal="5dp"
                                android:text=":"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/tvLblAlamat"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:text="Alamat"
                                android:textColor="@color/black" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/lytJmlitem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="end"
                            android:padding="10dp">

                            <TextView
                                android:id="@+id/tvJmlItemBottom"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/tv_titikd11"
                                android:gravity="end"
                                android:paddingEnd="5dp"
                                android:text="100 Item"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/tv_titikd11"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/tvLblitem"
                                android:gravity="center"
                                android:paddingHorizontal="5dp"
                                android:text=":"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/tvLblitem"
                                android:layout_width="120dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:text="Jumlah Item"
                                android:textColor="@color/black" />

                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayoutTllHrgbg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone"
                    app:layout_constraintBottom_toTopOf="@id/btnLayoutBottomSheet"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent">
                    <!--Total Harga 1-->
                    <RelativeLayout
                        android:id="@+id/ttlHrgLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/tvLbltotalhrg"
                            android:layout_width="120dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:text="Total Harga"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tv_titikd6ManualBook"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tvLbltotalhrg"
                            android:gravity="center"
                            android:paddingHorizontal="5dp"
                            android:text=":"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tvTotalhrg"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tv_titikd6ManualBook"
                            android:gravity="end"
                            android:paddingEnd="5dp"
                            android:text="Rp. 50.000"
                            android:textColor="@color/black" />
                    </RelativeLayout>
                    <!--Total Harga 2-->
                    <RelativeLayout
                        android:id="@+id/ttlHrg2Layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/tvLbltotalhrg2"
                            android:layout_width="120dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:text="Total Harga"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tv_titikd7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@id/tvLbltotalhrg2"
                            android:gravity="center"
                            android:paddingHorizontal="5dp"
                            android:text=":"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/tvTotalhrg2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_titikd7"
                            android:layout_alignParentLeft="true"
                            android:gravity="end"
                            android:paddingEnd="5dp"
                            android:text="Rp. 50.000"
                            android:textColor="@color/black" />
                    </RelativeLayout>
                </LinearLayout>
                <!--Bottom Sheet-->
                <RelativeLayout
                    android:id="@+id/btnLayoutBottomSheet"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginBottom="30dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:padding="5dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="70dp">

                                <LinearLayout
                                    android:id="@+id/layoutBtnSimpan"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                    <Button
                                        android:id="@+id/btnSimpan"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:backgroundTint="#2CA602"
                                        android:text="Simpan" />
                                </LinearLayout>

                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:padding="5dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="70dp">

                                <LinearLayout
                                    android:id="@+id/layoutBtnBatal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                    <Button
                                        android:id="@+id/btnBatal"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginHorizontal="5dp"
                                        android:backgroundTint="#A6A6A6"
                                        android:text="Batal" />
                                </LinearLayout>

                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </LinearLayout>


                    </LinearLayout>
                </RelativeLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/viewCollapse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:paddingTop="15dp"
                    android:paddingBottom="40dp">

                    <View
                        android:layout_width="30dp"
                        android:layout_height="5dp"
                        android:background="@drawable/btn_action"
                        android:backgroundTint="#777777" />
                </LinearLayout>
            </LinearLayout>


        </androidx.cardview.widget.CardView>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>